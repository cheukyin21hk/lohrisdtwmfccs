	public static void main(String args[])
	{
		Wave wave = new Wave("/Users/lohris/programming/FYP/Dev/testingworkplace/TestingDTWMCSS/src/res/wave2.wav");
		Wave wave1 = new Wave("/Users/lohris/programming/FYP/Dev/testingworkplace/TestingDTWMCSS/src/res/wave3.wav");
		Wave bellRinging = new Wave("/Users/lohris/programming/FYP/Dev/testingworkplace/TestingDTWMCSS/src/sound/bell-ringing-01.wav");
		Wave bellRinging2 = new Wave("/Users/lohris/programming/FYP/Dev/testingworkplace/TestingDTWMCSS/src/sound/bell-ringing-02.wav");
